{
  "expo": {
    "name": "CAN International Customer",
    "slug": "can-international-customer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app/nepal-can-intl-logo.png",
    "scheme": "can-international-customer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.canlogistic.internationalcustomer",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses camera for QR code scanning and product photos",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select product images",
        "NSLocationWhenInUseUsageDescription": "This app uses location for nearby stores and delivery tracking",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/app/nepal-can-intl-logo.png",
        "backgroundImage": "./assets/app/nepal-can-intl-logo.png",
        "monochromeImage": "./assets/app/nepal-can-intl-logo.png"
      },
      "versionCode": 1,
      "package": "com.canlogistic.internationalcustomer",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/app/nepal-can-intl-logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/app/nepal-can-intl-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/app/nepal-can-intl-logo.png",
            "backgroundColor": "#000000"
          },
          "backgroundColor": "#232323"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CAN International Customer to access your camera for QR code scanning"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow CAN International Customer to access your photo library to select images"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CAN International Customer to use your location for nearby stores and delivery tracking"
        }
      ],
      "expo-router",
      "expo-web-browser",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5e1fe59a-c0d9-4752-a55f-291a889253b9"
      }
    }
  }
}
